{
  "name": "interactive-launchpad",
  "version": "1.0.0",
  "description": "Standard kit for Mixer interactive",
  "scripts": {
    "start": "miix serve",
    "test": "tslint --type-check --project tsconfig.json \"src/**/*.{ts,tsx}\"",
    "fmt": "npm-run-all --silent fmt:style fmt:lint",
    "fmt:lint": "tslint --type-check --project tsconfig.json \"src/**/*.{ts,tsx}\" --fix",
    "fmt:style": "prettier --single-quote --trailing-comma all --parser typescript --print-width 100 --write \"src/**/*.{ts,tsx}\"",
    "publish:azure": "node bin/upload-to-azure.js"
  },
  "interactive": {
    "display": {
      "mode": "fixed-grid"
    }
  },
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "dependencies": {
    "decko": "^1.2.0",
    "eventemitter3": "^2.0.3",
    "intl-messageformat": "^2.1.0",
    "preact": "^8.2.1",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@mcph/miix-cli": "^0.2.0",
    "@mcph/miix-std": "^0.2.0",
    "@types/node": "^8.0.24",
    "awesome-typescript-loader": "^3.2.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "file-loader": "^0.11.2",
    "globby": "^7.1.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "tar": "^4.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.4.2",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1"
  }
}
